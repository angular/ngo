{
  "name": "webpack-app",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rimraf build && webpack -p --hide-modules",
    "build-no-ngo": "rimraf build-no-ngo && webpack -p --config=webpack.config.no-ngo.js --hide-modules",
    "serve": "lite-server --baseDir=dist",
    "pree2e": "npm run build",
    "e2e": "concurrently \"npm run serve\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "benchmark": "npm run build && npm run build-no-ngo && node benchmark.js"
  },
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "@ngtools/webpack": "1.3.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.45",
    "compression-webpack-plugin": "^0.4.0",
    "concurrently": "^3.4.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine": "^2.6.0",
    "lite-server": "^2.3.0",
    "ngo-loader": "file:..\\..",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "ts-node": "^3.0.4",
    "typescript": "~2.3.3",
    "webpack": "^2.5.1"
  }
}
