{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "outDir": "dist",
    "noEmitOnError": true,
    "strictNullChecks":true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "lib": [
      "es2015"
    ]
  },
  "exclude": [
    "node_modules/",
    "integration/"
  ]
}
