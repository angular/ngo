    "@types/node": "^7.0.5",
    "@types/source-map": "^0.5.0",
    "common-tags": "^1.4.0",
    "jasmine": "^2.6.0",
    "jasmine-spec-reporter": "^4.1.0",
    "rimraf": "^2.6.1",
    "ts-node": "^2.1.2",
    "tslint": "^5.3.2"
  }
